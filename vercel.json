{
  "rewrites": [
    {
      "source": "/ads.txt",
      "has": [
        {
          "type": "host",
          "value": "ficsitdb.com"
        }
      ],
      "destination": "/ads.txt"
    }
  ],
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "ficsitdb.com"
        }
      ],
      "destination": "https://www.ficsitdb.com/$1",
      "permanent": true
    }
  ]
}